    +script('raphael/raphael.min.js', 'plugin')
    +script('jvectormap/jquery-jvectormap-2.0.3.min.js', 'plugin')
    +script('jvectormap/jquery-jvectormap-world-mill-en.js', 'plugin')
    +script('peity/jquery.peity.min.js', 'plugin')

