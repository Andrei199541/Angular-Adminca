  #new-question-dialog.modal.fade(aria-labelledby='new-question-dialog', tabindex='-1', role='dialog')
    .modal-dialog(role='document')
      form.modal-content
        .modal-header.p-4
          h5.modal-title NEW QUESTION
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
        .modal-body.p-4
          .form-group.mb-4
            label.text-muted.mb-3 Category
            div
              label.radio.radio-outline-primary.radio-inline.check-single(data-toggle='tooltip', data-original-title='General')
                input(type='radio', name='cat', checked='')
                span.input-span
              label.radio.radio-outline-warning.radio-inline.check-single(data-toggle='tooltip', data-original-title='Payment')
                input(type='radio', name='cat')
                span.input-span
              label.radio.radio-outline-info.radio-inline.check-single(data-toggle='tooltip', data-original-title='Technical')
                input(type='radio', name='cat')
                span.input-span
              label.radio.radio-outline-danger.radio-inline.check-single(data-toggle='tooltip', data-original-title='Registration')
                input(type='radio', name='cat')
                span.input-span
              label.radio.radio-outline-blue.radio-inline.check-single(data-toggle='tooltip', data-original-title='Security')
                input(type='radio', name='cat')
                span.input-span
          .form-group.mb-4
            input.form-control.form-control-line(type='text', placeholder='Question')
          .form-group.mb-4
            textarea.form-control.form-control-line(placeholder='Answer of the question', rows='4')
        .modal-footer.justify-content-start.bg-primary-50
          button.btn.btn-primary.btn-rounded.mr-3(type='button') Submit


block page_styles
  style.
    .faq-list a[data-toggle="collapse"] {display:block; position:relative}
    .faq-list a[data-toggle="collapse"]:after {
      position: absolute;
      right: 0;
      top: 50%;
      transform: translateY(-50%);
      font-family: 'themify';
      content: "\e61a";
      speak: none;
      font-style: normal;
      font-weight: normal;
      font-variant: normal;
      text-transform: none;
      line-height: 1;
      -webkit-font-smoothing: antialiased;
    }
    .faq-list a[data-toggle="collapse"][aria-expanded=true]:after {
      content: "\e622";
    }
