extends ../layout.pug

block variables
  - var pageTitle = 'FAQ'
  - var pageUrl = 'faq.html'

block content
  .flexbox-b.mb-5
    span.mr-4.static-badge.badge-info
      i.ti-support
    div
      h5.font-strong FAQ
      .text-light Found 42 Questions in 5 categories
  .input-group-icon.input-group-icon-left.input-group-lg.mb-5
    span.input-icon.input-icon-right
      i.ti-search
    input.form-control.form-control-air.font-light.font-poppins.border-0(type='text', placeholder='Search ...', style='box-shadow:0 3px 6px rgba(10,16,20,.15);')
  .row
    .col-lg-3
      button.btn.btn-primary.btn-rounded.btn-block.mb-4(data-toggle='modal', data-target='#new-question-dialog')
        span.btn-icon
          i.la.la-plus-circle
          | New Question
      ul.list-group.list-group-divider.faq-tabs
        li.list-group-item
          a.active(href='#accordion-1', data-toggle='tab')
            i.badge-point.badge-primary.mr-3
            | General
        li.list-group-item
          a(href='#accordion-2', data-toggle='tab')
            i.badge-point.badge-warning.mr-3
            | Payment
        li.list-group-item
          a(href='#accordion-3', data-toggle='tab')
            i.badge-point.badge-info.mr-3
            | Technical issues
        li.list-group-item
          a(href='#accordion-3', data-toggle='tab')
            i.badge-point.badge-danger.mr-3
            | Registration
        li.list-group-item
          a(href='#accordion-3', data-toggle='tab')
            i.badge-point.badge-blue.mr-3
            | Security
