{
  "compile": {
      "jsUglify": true,
      "cssMinify": true
  },
  "path": {
      "src": "../../src",
      "output": "../../dist",
      "bower_components": "../../src/bower_components",
      "vendors": "../../src/vendors"
  },
  "build": {
      "admin_1": {
          "js": {
              "src": [
                  "@{path.src}/js/app/app.js",
                  "@{path.src}/js/theme/admin_1/layout.js"
              ],
              "output": "@{path.output}/admin_1/html/assets/js"
          },
          "sass": {
              "src": {
                  "bundle": [
                      "@{path.src}/sass/components/components.scss",
                      "@{path.src}/sass/layouts/admin_1/layout.scss",
                      "@{path.src}/sass/vendors/vendors.scss"
                  ],
                  "themes": "@{path.src}/sass/layouts/admin_1/themes/*.scss",
                  "pages": "@{path.src}/sass/layouts/admin_1/pages/*.css"
              },
              "output": {
                  "bundle": "@{path.output}/admin_1/html/assets/css",
                  "themes": "@{path.output}/admin_1/html/assets/css/themes",
                  "pages": "@{path.output}/admin_1/html/assets/css/pages"
              }
          },
          "pug": {
            "src": "@{path.src}/pug/admin_1/pages/*.pug",
            "output": "@{path.output}/admin_1/html"
          }
      },
